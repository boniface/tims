<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="hashthrimsPU" transaction-type="RESOURCE_LOCAL">
    <property name="hibernate.cache.provider_class" value="net.sf.ehcache.hibernate.SingletonEhCacheProvider"/>
    <property name="hibernate.cache.use_query_cache" value="true"/>
    <property name="hibernate.cache.use_second_level_cache" value="true"/>
    <property name="hibernate.generate_statistics" value="true"/>
    <class>com.hashthrims.domain.traininglist.MentoringObjective</class>
    <class>com.hashthrims.domain.traininglist.MentoringSessionObjective</class>
    <class>com.hashthrims.domain.traininglist.MentoringSessionTheme</class>
    <class>com.hashthrims.domain.traininglist.MentoringSessionType</class>
    <class>com.hashthrims.domain.traininglist.SessionAreasOfStrengthening</class>
    <shared-cache-mode>ALL</shared-cache-mode>
  </persistence-unit>
</persistence>
